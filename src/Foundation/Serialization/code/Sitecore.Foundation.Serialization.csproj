<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net471</TargetFramework>
        <RootNamespace>Sitecore.Foundation.Serialization</RootNamespace>
        <AssemblyName>Sitecore.Foundation.Serialization</AssemblyName>
        <TargetsInstanceRole>Website</TargetsInstanceRole>
    </PropertyGroup>
    <ItemGroup>
        <Content Include="$(NuGetPackageRoot)\Unicorn\4.0.8\content\**\*">
            <CopyToOutputDirectory>Never</CopyToOutputDirectory>
            <CopyToPublishDirectory>Always</CopyToPublishDirectory>
        </Content>
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Unicorn" Version="4.0.8" />
        <PackageReference Include="Unicorn.Roles" Version="4.0.8" />
        <PackageReference Include="Unicorn.Users" Version="4.0.8" />
    </ItemGroup>
</Project>